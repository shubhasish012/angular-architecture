/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "extends": "./tsconfig.base.json",
  "compilerOptions": {
    "paths": {
      "@app/*":[
        "app/*"
      ],
      "@src/*":[
        "*"
      ]
    },
    "outDir": "./out-tsc/app",
    "experimentalDecorators": true,
    "types": []
  },
  "files": [
    "src/main.ts",
    "src/polyfills.ts"
  ],
  "include": [
    "src/**/*.d.ts"
  ]
}
